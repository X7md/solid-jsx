{
  "name": "solid-jsx",
  "version": "0.3.0",
  "description": "",
  "source": "src/jsx-runtime.tsx",
  "main": "./dist/jsx-runtime.js",
  "module": "./dist/jsx-runtime.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/jsx-runtime.js",
      "default": "./dist/jsx-runtime.mjs"
    },
    "./jsx-runtime": {
      "require": "./dist/jsx-runtime.js",
      "default": "./dist/jsx-runtime.mjs"
    },
    "./jsx-dev-runtime": {
      "require": "./dist/jsx-runtime.js",
      "default": "./dist/jsx-runtime.mjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "types": "./dist/jsx-runtime.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "prepare": "husky install",
    "build": "rollup -c && echo '{\"type\":\"module\"}' > dist/package.json && tsc --emitDeclarationOnly",
    "prepublishOnly": "pnpm build"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/high1/solid-jsx.git"
  },
  "keywords": [
    "solid",
    "mdx"
  ],
  "author": "high1 <high1@mailfence.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/high1/solid-jsx/issues"
  },
  "homepage": "https://github.com/high1/solid-jsx#readme",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-preset-solid": "^1.3.2",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.64.0",
    "solid-js": "^1.3.3",
    "typescript": "^4.5.4"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{js,css,json,md,jsx,yml,yaml}": "prettier --ignore-path .gitignore --write"
  }
}
