{
  "name": "solid-jsx",
  "version": "0.4.0",
  "description": "",
  "source": "src/jsx-runtime.ts",
  "main": "./dist/jsx-runtime.js",
  "module": "./dist/jsx-runtime.mjs",
  "exports": {
    ".": {
      "import": "./dist/jsx-runtime.mjs",
      "require": "./dist/jsx-runtime.js"
    },
    "./jsx-runtime": {
      "import": "./dist/jsx-runtime.mjs",
      "require": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "import": "./dist/jsx-runtime.mjs",
      "require": "./dist/jsx-runtime.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "types": "./dist/jsx-runtime.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "prepare": "husky install",
    "build": "tsup src/jsx-runtime.ts --dts --format cjs,esm",
    "prepublishOnly": "pnpm build"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/high1/solid-jsx.git"
  },
  "keywords": [
    "solid",
    "mdx"
  ],
  "author": "high1 <high1@mailfence.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/high1/solid-jsx/issues"
  },
  "homepage": "https://github.com/high1/solid-jsx#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.2.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "tsup": "^5.11.11",
    "typescript": "^4.5.4"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{js,css,json,md,jsx,yml,yaml}": "prettier --ignore-path .gitignore --write"
  },
  "dependencies": {
    "solid-js": "^1.3.3"
  }
}
